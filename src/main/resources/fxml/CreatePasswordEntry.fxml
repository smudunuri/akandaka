<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<GridPane prefHeight="272.0" prefWidth="546.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.antosara.akandaka.CreatePasswordEntryController">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" maxWidth="284.0" minWidth="10.0" prefWidth="106.0" />
		<ColumnConstraints hgrow="SOMETIMES" maxWidth="472.0" minWidth="10.0" prefWidth="472.0" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" valignment="CENTER" vgrow="NEVER" />
		<RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" valignment="CENTER" vgrow="NEVER" />
		<RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" valignment="CENTER" vgrow="NEVER" />
		<RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" valignment="CENTER" vgrow="NEVER" />
		<RowConstraints maxHeight="120.0" minHeight="120.0" prefHeight="120.0" valignment="CENTER" vgrow="SOMETIMES" />
		<RowConstraints maxHeight="228.0" minHeight="10.0" prefHeight="61.0" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label text="Title" />
		<TextField fx:id="title" GridPane.columnIndex="1" />
		<Label text="User name" GridPane.rowIndex="1" />
		<Label text="Password" GridPane.rowIndex="2" />
		<Label text="URL" GridPane.rowIndex="3" />
		<Label text="Notes" GridPane.rowIndex="4" />
		<TextField fx:id="userName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
		<PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="2" />
		<TextField fx:id="url" GridPane.columnIndex="1" GridPane.rowIndex="3" />
		<TextArea fx:id="misc" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
			<GridPane.margin>
				<Insets top="3.0" />
			</GridPane.margin>
		</TextArea>
		<HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="5">
			<children>
				<Button fx:id="save" mnemonicParsing="false" onAction="#save" text="Save">
					<HBox.margin>
						<Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
					</HBox.margin>
				</Button>
				<Button fx:id="clear" mnemonicParsing="false" onAction="#clear" text="Clear">
					<HBox.margin>
						<Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
					</HBox.margin>
				</Button>
			</children>
		</HBox>
	</children>
</GridPane>
